{
    "parser": "babel-eslint",
    "env": {
        "mocha": true,
        "node": true,
        "es6": true
    },
    "settings": {
        "ecmascript": 6
    },
    "rules": {
        "strict": 0,
        "quotes": 0,
        "no-unused-vars": 0,
        "camelcase": 0,
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "no-process-exit": 0,
        "no-shadow": 0,
        "consistent-return": 0,
        "no-extra-parens": 1,
        "no-use-before-define": 0,
        "no-unused-expressions": 0
    }
}
